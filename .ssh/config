# Default Github
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_webfilings

Host github-wf
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_webfilings

Host heroku.com
    HostName heroku.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_bitbucket

Host amazonaws.com
    HostName amazonaws.com
    User ubuntu
    IdentityFile ~/.ssh/michael.pem
